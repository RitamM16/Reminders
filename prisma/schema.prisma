datasource db {
    provider    = "sqlite"
    url         = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    @@map("user")
    id      Int     @id @default(autoincrement())
    email   String  @unique
    name    String
    password String
    createAt DateTime @default(now()) @map("created_at")
    updatedAt DateTime @updatedAt @map("updated_at")
}