    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch API Server",
            "program": "${workspaceFolder}\\src\\API_Server\\index.ts",
            "sourceMaps": true,
            "trace": "all",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "envFile": "${workspaceFolder}/.env",
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Worker",
            "program": "${workspaceFolder}\\src\\Worker\\index.ts",
            "sourceMaps": true,
            "trace": "all",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "outputCapture": "std"
        }
    ]
}